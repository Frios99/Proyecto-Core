@model IEnumerable<MVCCore_Francis.Models.Historial>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Fecha)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Historia)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Titulo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UsuarioID)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Fecha)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Historia)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Titulo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UsuarioID)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.HistorialID">Editar</a> |
                <a asp-action="Details" asp-route-id="@item.HistorialID">Detalles</a> |
                <a asp-action="Delete" asp-route-id="@item.HistorialID">Eliminar</a> |
                <a asp-action="Recomendacion" asp-route-id="@item.HistorialID">Recomendacion</a>
            </td>
        </tr>
}
    </tbody>
</table>
